            <class name="com.vtiger.tests.PartnersOption"></class>
            <class name="com.vtiger.tests.NavigationBar"></class>
            <class name="com.vtiger.tests.ContactUs"></class>
            <class name="com.vtiger.tests.BlogFeature"></class>
