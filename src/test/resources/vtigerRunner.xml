            <class name="com.vtiger.tests.DaysFreeTrial"></class>
            <class name="com.vtiger.tests.ExploreAllFeature"></class>
            <class name="com.vtiger.tests.LoginTest"></class>
            <class name="com.vtiger.tests.LogoIcon"></class>
            <class name="com.vtiger.tests.MarketingArticlesList"></class>
            <class name="com.vtiger.tests.OneViewProductList"></class>
